{
  "compilerOptions": {
    "outDir": "./tmp/",
    "sourceMap": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": false,
    "noEmitOnError": true
  },
  "include": [
    "./tool/"
  ]
}
